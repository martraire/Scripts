   foreach it (`seq 1 1 100`)
	foreach it2(`seq 28 1 30`)
	@ itgerbe = ${it} + 267
	set file = QSGJET_E${en}_53-60_${it}_${it2}
	rm -f ${file}.sh
	touch ${file}.sh

	echo "source ${augerdb}" >> ${file}.sh
	echo "${itgerbe} ${it}"	
        echo "SdbGetShower ${shower_list} ${itgerbe} ${itgerbe}" >> ${file}.sh
	#echo  mv $LISTFILE[$it].part ${workdir2}/. >> ${file}.sh
	echo  mv $LISTFILE[${it}].part QSGJET_E${en}_53-60_${it}_${it2}.part >> ${file}.sh
	echo "${corstoroot} QSGJET_E${en}_53-60_${it}_${it2}.part QSGJET_E${en}_53-60_${it}_${it2}.root" >> ${file}.sh
	#echo  mv QSGJET_E${en}_53-60_${it}_${it2}.root ${workdir2}/. >> ${file}.sh
	echo cp ${default_file} ./ >> ${file}.sh
	echo "${augersim} QSGJET_E${en}_53-60_${it}_${it2}.root" >> ${file}.sh
	echo  mv Sim_QSGJET_E${en}_53-60_${it}_${it2}.root ${workdir2}/Sim_QSGJET_E${en}_53-60_${it}_${it2}.root >> ${file}.sh 

	 chmod +x ${file}.sh
	end
    end
end




